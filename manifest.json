{
    "manifest_version": 3,
    "name": "Tab Switcher",
    "version": "1.0",
    "description": "Switch tab easily.",
    "permissions": [
        "activeTab",
        "tabs",
        "scripting",
        "windows"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "images/icon-16.png"
    },
    
    "commands": {
        "open-search-container": {
            "suggested_key": {
                "default": "Ctrl+Shift+F"
            },
            "description": "Activate search container"
        }
    }
}